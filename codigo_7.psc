Algoritmo codigo_7
	definir contador  Como Entero
	contador <- 1 
	suma <- 0 
	definir num como entero 
	
	mientras contador <= 5 Hacer
		escribir " digite un número: "
		leer num 
		suma <- suma + num 
		contador <- contador + 1
	FinMientras
	
	escribir  " la suma total, es: ", suma 
FinAlgoritmo
